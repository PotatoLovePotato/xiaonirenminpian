<!-- 列表 -->
<view class="">
<view class="cardse EnterpriseInfo">
    <image class="sign" src="/image/jiao_03.jpg"></image>
    <view class="cardseimg"><image src="http://card.xiaoniren.cn{{firmlist.logo}}"></image></view>
    <view class="cardser">
      <view class="cardsera">{{firmlist.name}}</view>
      <view class="cardsera">
        <view class=""><image src="/image/name.png"></image><text>{{firmlist.username}}</text></view>
        <view class=""><image src="/image/2_03.jpg"></image><text>{{firmlist.registered_capital}}</text></view>
      </view>
      <view class="cardserb cardserbb"><image src="/image/tel.png"></image> <text> {{firmlist.phone}}</text></view>
      <view class="cardserb cardserbb"><image src="/image/yew4.png"></image> <text> {{firmlist.business}}</text></view>
    </view>
    <view class="intro">{{firmlist.title}}</view>
    <view class="liang">
      <view class="lianglist"><image class="sshoucimg" src="/image/k.png"></image> <text> {{firmlist.clicks}}人看过</text></view>
      <view class="lianglist"><image class="sshoucimg" src="/image/s.png"></image> <text> {{firmlist.collect}}收藏</text></view>
    </view>
</view>
<view class="conz"></view>
